{% extends "./base/base.njk" %}
{% block body %}
    <h1>Create Robot</h1>
    <form action="/api/createRobot" method="POST" enctype="application/x-www-form-urlencoded">
        <p>Name robot</p>
        <input type="text" name="name">
        <p>Body robot</p>
        <input type="text" name="body">
        <p>Weapons robot</p>
        <input type="text" name="weapons">
        <button type="submit">Create</button>
    </form>
{% endblock %}